<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">创建比王</h1>
  </div>
  <!-- /.col-lg-12 -->
</div>

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        比王的信息
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-lg-12">
            <%= form_for @player, url: admin_players_path, method: :post, html: {autocomplete: 'off'} do |f| %>
               <%= render "admin/common/error_messages", :target => @player %>
               <%= render :partial => 'admin/players/form', :locals => { :f => f } %>
            <% end %>
          </div>
        </div>
        <!-- /.row (nested) -->
      </div>
    </div>
  </div>
</div>
